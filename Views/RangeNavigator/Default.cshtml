    <script src="~/Scripts/datasource.js"></script>
    <script>
        var data = bitCoinData;
        var selectedTheme = themeName ? themeName : 'Fluent2';
        var theme = (selectedTheme.charAt(0).toUpperCase() + selectedTheme.slice(1)).replace(/-dark/i, "Dark").replace(/contrast/i, 'Contrast').replace(/-highContrast/i, 'HighContrast').replace(/5.3/i, '5');
        var defaultthemes = ['bootstrap5', 'bootstrap5dark', 'tailwind', 'tailwinddark', 'material', 'materialdark', 'bootstrap4', 'bootstrap', 'bootstrapdark', 'fabric', 'fabricdark', 'highcontrast', 'fluent', 'fluentdark', 'material3', 'material3dark', 'fluent2', 'fluent2highcontrast', 'fluent2dark', 'tailwind3', 'tailwind3dark'];
        var borderColor = ['#FD7E14', '#FD7E14', '#5A61F6', '#8B5CF6', '#00bdae', '#9ECB08', '#a16ee5', '#a16ee5', '#a16ee5', '#4472c4', '#4472c4', '#79ECE4', '#1AC9E6', '#1AC9E6', '#6355C7', '#4EAAFF', '#6200EE', '#9BB449', '#9BB449', '#2F4074', '#8029F1'];
        var regionColor = ['rgba(253, 126, 20, 0.3)', 'rgba(253, 126, 20, 0.3)',
            'rgba(90, 97, 246, 0.3)', 'rgba(139, 92, 246, 0.3)', 'rgba(0, 189, 174, 0.3)', 'rgba(22, 22, 22, 0.6)',
            'rgba(161, 110, 229, 0.3)', 'rgba(161, 110, 229, 0.3)', 'rgba(22, 22, 22, 0.6)', 'rgba(68, 114, 196, 0.3)',
            'rgba(22, 22, 22, 0.6)', 'rgba(121, 236, 228, 0.3)', 'rgba(26, 201, 230, 0.3)', 'rgba(26, 201, 230, 0.6)', 'rgba(99, 85, 199, 0.3)', 'rgba(78, 170, 255, 0.6)', 'rgba(98, 0, 238, 0.3)', 'rgba(155, 180, 73, 0.3)', 'rgba(155, 180, 73, 0.3)', 'rgba(47, 64, 116, 0.3)', 'rgba(128, 41, 241, 0.3)'];
        var load = function (args) {
            args.rangeNavigator.theme = theme;
            args.rangeNavigator.width = ej.base.Browser.isDevice ? "100%" : "80%";
            args.rangeNavigator.series[0].dataSource = data;
            args.rangeNavigator.series[0].border = { width: 2, color: borderColor[defaultthemes.indexOf(theme.toLowerCase())] };
            args.rangeNavigator.series[0].fill = 'url(#' + theme.toLowerCase() + '-gradient-chart)';
            args.rangeNavigator.value = [new Date('2017-09-01'), new Date('2018-02-01')];
            args.rangeNavigator.navigatorStyleSettings.unselectedRegionColor = 'transparent';
            args.rangeNavigator.navigatorStyleSettings.selectedRegionColor = regionColor[defaultthemes.indexOf(theme.toLowerCase())];
        }
    </script>
}
